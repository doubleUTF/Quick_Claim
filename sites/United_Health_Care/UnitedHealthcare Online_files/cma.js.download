 function hideDiv(divName){
   var divNme = document.getElementById(divName);
   if(divNme != null){
   //var objDivStyle = divNme.style;
   var objDivStyle = eval('document.all.'+divName+'.style'); //Change for Mozilla #27
   if(objDivStyle != null)
	   objDivStyle.visibility = 'hidden';
   }
 }
 
 function visibleDiv(divName){
   var divNme = document.getElementById(divName);
   if(divNme != null){
	   var objDivStyle = eval('document.all.'+divName+'.style'); //Change for Mozilla #27
	   if(objDivStyle != null) {
	    	objDivStyle.visibility = 'visible';
			if(divName == 'printLink2') {
				objDivStyle.display ='block';
	        }else{
	        	if(divName == 'printLink2Popup') {
					objDivStyle.display ='block';
	            }
	        }
	               
		} 
 	}
 }
 
 function disableForms() {
    if(document.getElementById('aselect')){
    	document.getElementById('aselect').href="";
    }
   if(document.getElementById('radjustment') != null){
       	document.getElementById('radjustment').disabled = true;
    }
   if(document.getElementById('sadjustment') != null){
       	document.getElementById('sadjustment').disabled = true;
    }
	if(document.getElementById('Submit2') != null){
   		document.getElementById('Submit2').disabled = true;
   }        
   if(document.getElementById('Submit3') != null){
   		document.getElementById('Submit3').disabled = true;
   }
   
      if(document.getElementById('ok') != null){
   		document.getElementById('ok').disabled = true;
   }
       
	var m = document.getElementsByTagName("input");
    for(i=0; i<m.length; i++){
	    var tempobj = m[i]
		var type=tempobj.type.toLowerCase();
		if(type == "submit" || type== "reset" || type== "button")
		    tempobj.disabled = true;
    }   
    var formLen = document.forms.length;
    for(i=0; i<formLen; i++){
	if (document.all || document.getElementById) {
		var elemLen = document.forms[i].length;
		
		for (j = 0; j < elemLen; j++) {
			var tempobj = document.forms[i].elements[j];

			if (tempobj.type.toLowerCase() == "submit" 
			|| tempobj.type.toLowerCase()=="radio" || tempobj.type.toLowerCase()=="checkbox" || tempobj.type.toLowerCase() == "button"){
				tempobj.disabled = true;
			} else if (tempobj.type.toLowerCase() =="text" || tempobj.type.toLowerCase() == "textarea"){
				tempobj.disabled = false;
				tempobj.readOnly=true;
				tempobj.onfocus = function(){
					this.blur();
				};
			} else if (tempobj.type.toLowerCase() == "select-one"){
				tempobj.disabled = false;
				tempobj.onfocus = function(){
					this.blur(); 
					alert('You can not change value on printer friendly page.');
				};
			}
		}
		
		
	}
	
   }//for
   
