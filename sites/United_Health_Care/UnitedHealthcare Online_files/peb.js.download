function checkAndUncheckAll(fieldName){
  

	if(value == "unCheck"){
   	  for(var i=0; i<fieldName.length; i++){
		fieldName[i].checked = true;
	  }
	  value = "check";
	
	}else if(value == "check"){
   	  for(var i=0; i<fieldName.length; i++){
		fieldName[i].checked = false;
          }
	  value="unCheck";

	}
	
	 el = document.getElementById("select");
	if (el.style["display"] == 'block')
	   el.style["display"] = 'none';
	else
		el.style["display"] = 'block';
	 ell = document.getElementById("unSelect");
	if (ell.style["display"] == 'block')
	   ell.style["display"] = 'none';
	else
	   ell.style["display"] = 'block';
	
 }


function checkUpdate()
{
	var cnt=0;
	var el=document.forms[1].elements;
	for(i=0;i<el.length;i++){			
			if(el[i].type=='checkbox' && el[i].checked)
			{
				cnt++;
			}
	}
	if(cnt==0)
	{
		el = document.getElementById("select");
		if (el.style["display"] == 'none')
		 checkAndUncheckAll(document.enrolleeSummary.patientCheck);
	}
}

