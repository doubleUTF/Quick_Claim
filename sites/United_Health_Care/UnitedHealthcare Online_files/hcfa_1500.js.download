function HcfaRequiredField(fieldName, lineNum, fieldMsg) {
	this.fieldName = fieldName;
	this.lineNum = lineNum;
	this.fieldMsg = fieldMsg;

}

function getIndexByFieldName(array,fieldName) {

	for(var i=0; i<array.length;i++) {
	
		if (array[i].fieldName == fieldName) {
			return i;
		}
	}

	return -1;

}


function showAlertBox(fieldType,fieldName) {
	var msg;
	var iIndex = getIndexByFieldName(requiredFieldsMsg,fieldName);
	//alert("inside showalertbox "+ fieldName +",line " + iIndex);


	if ( iIndex >=0) {
		msg = requiredFieldsMsg[iIndex].fieldMsg;

		var linenum = requiredFieldsMsg[iIndex].lineNum;

		if (isNaN(linenum)) {

			msg = "Line " + linenum + " " + msg;
		}
	} else {
		// this is only for debug and test only
		// need to put this field into requiredFieldsMsg
		// if this message pop up
		msg = fieldName + " required field need to be set up";
	}


	alert(msg);
	

	if (fieldType == "text" || fieldType == "select" ||  fieldType == "checkbox") {
		
		eval("document.mainform." + fieldName + ".focus()");
		
	} else if ( fieldType == "radio") {
	
		eval("document.mainform." + fieldName + "[0].focus()");
	}
	
	
}









	

		
		
