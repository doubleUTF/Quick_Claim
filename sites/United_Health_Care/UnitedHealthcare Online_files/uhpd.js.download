function showPinValidation()
{	

	var url,params;
	var frm = document.premiumPinValidationForm;
	params= "userSeqNo=" + frm.userSeqNo.value+ "&" + "forwardPage=" + frm.forwardPage.value;

	url="premiumExptTblcheck.do?function=showpinvalidation";

	url += "&" + params;
	
	doAjaxSubmit(url);
}
function premiumExptTblCheck()
{	
	var url,params;
	var frm = document.premiumPinValidationForm;
	
	params= "userSeqNo=" + frm.userSeqNo.value + "&" + "forwardPage=" + frm.forwardPage.value;
	url="premiumExptTblcheck.do?function=showvalidationlink";

	url += "&" + params;

	doAjaxSubmit(url);
}



function hideShowNotifyDiv(obj)
{
	var emailPrefsDiv = document.getElementById('emailPrefs');
	var faxPrefsDiv = document.getElementById('faxPrefs');
	var usMailPrefsDiv = document.getElementById('usmailPrefs');
	var email = document.getElementById('email');
	var emailConfirm = document.getElementById('emailConfirm');
	var areaCode = document.getElementById('areaCode');
	var exchangeCode = document.getElementById('exchangeCode');
	var subscriberNum = document.getElementById('subscriberNum');

	if(obj.value=="email")
	{
			emailPrefsDiv.style.display = 'block';
			faxPrefsDiv.style.display = 'none';
			usMailPrefsDiv.style.display = 'none';
			areaCode.value = '';
			exchangeCode.value = '';
			subscriberNum.value = '';
	}
	else if(obj.value=="fax")
	{
			emailPrefsDiv.style.display = 'none';
			faxPrefsDiv.style.display = 'block';
			usMailPrefsDiv.style.display = 'none';
			email.value = '';
			emailConfirm.value = '';
	}
	else if(obj.value=="U.S. Mail")
	{
			emailPrefsDiv.style.display = 'none';
			faxPrefsDiv.style.display = 'none';
			usMailPrefsDiv.style.display = 'block';
			areaCode.value = '';
			exchangeCode.value = '';
			subscriberNum.value = '';
	}
}

function inputFax(obj)
{
	if(obj.value.length==3)
	{
		if(obj.name=='areaCode')
		{
				var faxExchangeCode = document.getElementsByName('exchangeCode');
				faxExchangeCode[0].focus();
		}
		else if(obj.name=='exchangeCode')
		{
				var subscriberNum = document.getElementsByName('subscriberNum');
				subscriberNum[0].focus();
		}
	}
}

function showHelp(obj)
{
	var win = window.open("/b2c/CmaAction.do?viewKey=Help#"+obj.toString(), 'formresult', 'scrollbars=yes,menubar=no,height=600,location=no,width=800,resizable=no,toolbar=no,status=no'); 
	win.focus();
}
function sortBy(col) {
 	document.physicianGroupSelectorForm.param.value="processSort"
 	
 	 var lastSortCol = document.physicianGroupSelectorForm.PRIM_KEY.value;
 	 var lastSortOrder =document.physicianGroupSelectorForm.ASC_SORT.value;	 	 
 	 if (col == lastSortCol && lastSortOrder == "true")  {
 		document.physicianGroupSelectorForm.ASC_SORT.value = "false"; 			
 	 } else {
 		document.physicianGroupSelectorForm.PRIM_KEY.value=col;
 		document.physicianGroupSelectorForm.ASC_SORT.value = "true";
 	 }
 	 document.physicianGroupSelectorForm.action="/b2c/phyGrpSelection.do";
	// alert(document.physicianGroupSelectorForm.action);
 	 document.physicianGroupSelectorForm.submit();
}

function enableValidatePIN()
	{	
		var textBoxElem = document.premiumPinValidationForm.pinNo2;
		var textBoxElem1 = document.premiumPinValidationForm.pinNo1;
		var submitButton = document.getElementById('submitButton');
		if(((textBoxElem1.value.length==0)  || (textBoxElem1.value=="")) && ((textBoxElem.value.length==0)  || (textBoxElem.value==""))){		
				submitButton.className = 'disabledButton';	
				submitButton.disabled=true;	
		}
		else{
				//alert('dfdfa');
				submitButton.disabled=false;
				//alert('adfsfdsfdsfdsd');
				submitButton.className='button';
		}
		
	}
	
function fnPinSubmitForm() {
	//alert("in fnPinSubmitForm()");
   document.premiumPinValidationForm.submitFromConfirmPage.value="Y";
    if  (document.premiumPinValidationForm.forwardPage.value == "caseA"){
   document.premiumPinValidationForm.action = "/b2c/pinValidationCaseA.do";
   } else if (document.premiumPinValidationForm.forwardPage.value == "caseC1" || document.premiumPinValidationForm.forwardPage.value == "caseC2"){
   document.premiumPinValidationForm.action = "/b2c/pinValidationCaseC.do";
   } 
	document.premiumPinValidationForm.submit();
	
}

function fnPinCancel() {
	document.getElementById('alertSave').style.visibility='hidden';
	disableButtonCall();
	//alert("here");
	document.premiumPinValidationForm.pinNo1.value = "";
	document.premiumPinValidationForm.pinNo2.value = "";
}
function disableButtonCall() {
	var submitButton = document.getElementById('submitButton');
	submitButton.disabled = true;
}

