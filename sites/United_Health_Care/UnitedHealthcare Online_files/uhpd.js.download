  function show_cancel_popup(divbox){
		var alertBox=document.getElementById(divbox);
			alertBox.style.left = (document.body.clientWidth/2) - (alertBox.offsetWidth/2);			
		//alertBox.style.top = (document.body.scrollTop) + (document.body.clientHeight/2) - (alertBox.offsetHeight/2);
			alertBox.style.visibility="visible";
			
		event.returnValue=false;
		return false;
	}  
	
	
	function cancel_popup(divbox,tabswitch){
		var alertBox=document.getElementById(divbox);		
		alertBox.style.visibility="hidden";			
		var selectedTab=document.getElementById("selected");		
		if (tabswitch=='N') {
			selectedTab.value="";
		}		
		event.returnValue=false;
		return false;
	}
	
