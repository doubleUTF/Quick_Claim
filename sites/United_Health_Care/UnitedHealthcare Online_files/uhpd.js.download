function showDateValidationPopUp(dateValidationMsg) {
	if (window.showModalDialog)
	{
		window.showModalDialog('/b2c/reconDateValidation.do?dateValidationMsg='+dateValidationMsg, window,'dialogWidth:360px;dialogHeight:170px;status=no;unadorned=no;scroll=no;help:no');
	}
	else
	{
		window.open('/b2c/reconDateValidation.do?dateValidationMsg='+dateValidationMsg,window,'height=170,width=360,toolbar=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no ,modal=yes');
	}
}

